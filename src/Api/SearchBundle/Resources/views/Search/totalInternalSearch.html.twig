{% extends '::videos.html.twig' %}
{% block title %}{{ 'Search by keyword'|trans }}{% endblock%}
{% block javascripts %}  
    {{ parent() }}
<script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
<script>
  (function() {
var cx = 'your cx key'; //create your cx key in https://cse.google.com/cse/all
    var gcse = document.createElement('script'); gcse.type = 'text/javascript'; gcse.async = true;
    gcse.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') +
        '//www.google.com/cse/cse.js?cx=' + cx;
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(gcse, s);
  })();
</script>
<script type="text/javascript">
  function executeQuery() {
    var input = document.getElementById('cse-search-input-box-id');
    var element = google.search.cse.element.getElement('searchresults-only0');
    if (input.value == '') {
      element.clearAllResults();
    } else {
      element.execute(input.value);
    }
    return false;
  }
</script>
{% endblock javascripts %}


{% block body %}
    <h2>{{ 'Search by keyword in jobfreed.com'|trans }}</h2>
<form onsubmit="return executeQuery();" id="cse-search-box-form-id">    
<div class="col-lg-12">
    <div class="input-group">
        <input id="cse-search-input-box-id" class="form-control" autocomplete="on" placeholder="{{ 'Search by keyword... Autocompletion : enter a few words please'|trans }}"> 
        <span class="input-group-btn">
            <button type="submit" class="btn btn-default" type="button"><span class="fa fa-search fa-lg"></span></button>
        </span>
    </div><!-- /input-group -->
</div><!-- /.col-lg-6 -->
</form>
    <gcse:searchresults-only></gcse:searchresults-only>

{% endblock %}
